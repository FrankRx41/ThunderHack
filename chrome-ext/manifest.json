{
  "manifest_version": 2,

  "name": "迅雷离线地址获取工具",
  "description": "迅雷离线地址获取工具 Author: Chion82",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "<all_urls>",
    "cookies",
    "activeTab",
    "tabs"
  ],

  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
